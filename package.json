{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "npx vite",
    "prebuild": "npx tsx scripts/generateSitemapFromDB.ts && cross-env NODE_ENV=production npx tsx scripts/generateRobotsTxt.ts && node scripts/generateRedirectsSSG.js",
    "build": "npx vite build && node scripts/inject-async-css.js && node scripts/generateThematicPages.js && node scripts/generateOptimizedStaticPages.js && node scripts/generateOptimizedThematicPages.js && node scripts/generateComprendrePage.js && node scripts/generateIrokoZenStaticPage.js && node scripts/inject-env-vars.js",
    "lint": "eslint .",
    "preview": "npx vite preview",
    "force-reload": "node scripts/force-reload-simple.cjs",
    "deploy": "npm run build && gh-pages -d dist",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.2",
    "@supabase/supabase-js": "^2.58.0",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.2",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "recharts": "^3.4.1",
    "remark-gfm": "^4.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/node": "^24.7.0",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.18",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "gh-pages": "^6.3.0",
    "globals": "^15.9.0",
    "pdf-parse": "^1.1.4",
    "postcss": "^8.4.35",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.21"
  }
}
